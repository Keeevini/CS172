pt type=\"application/ld+json\">{\"@context\":\"https:\\/\\/schema.org\",\"@type\":\"Article\",\"name\":\"Search-oriented architecture\",\"url\":\"https:\\/\\/en.wikipedia.org\\/wiki\\/Search-oriented_architecture\",\"sameAs\":\"http:\\/\\/www.wikidata.org\\/entity\\/Q7441612\",\"mainEntity\":\"http:\\/\\/www.wikidata.org\\/entity\\/Q7441612\",\"author\":{\"@type\":\"Organization\",\"name\":\"Contributors to Wikimedia projects\"},\"publisher\":{\"@type\":\"Organization\",\"name\":\"Wikimedia Foundation, Inc.\",\"logo\":{\"@type\":\"ImageObject\",\"url\":\"https:\\/\\/www.wikimedia.org\\/static\\/images\\/wmf-hor-googpub.png\"}},\"datePublished\":\"2006-10-16T15:45:03Z\",\"dateModified\":\"2015-06-13T04:24:35Z\",\"headline\":\"concept in Information Technology\"}</script>\n<script>(RLQ=window.RLQ||[]).push(function(){mw.config.set({\"wgBackendResponseTime\":174,\"wgHostname\":\"mw1352\"});});</script>\n</body></html>",
